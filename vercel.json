{
    "name": "my-express-app",
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "./index.js"
        }
    ],
    "env": {
        "DB_CONNECTION_STRING": "$DB_CONNECTION_STRING"
    }
}
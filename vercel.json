{
    "name": "my-express-app",
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/products",
            "dest": "index.js"
        },
        {
            "src": ".*",
            "dest": "index.js"
        }
    ],
    "env": {
        "DB_CONNECTION_STRING": "$DB_CONNECTION_STRING"
    }
}